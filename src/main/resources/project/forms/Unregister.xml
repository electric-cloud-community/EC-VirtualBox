        <documentation>Flag to specify whether or not to delete files associated to the virtual machine.</documentation>
        <required>0</required>
        <type>checkbox</type>
        <checkedValue>1</checkedValue>
        <uncheckedValue>0</uncheckedValue>
    </formElement>
    <formElement>
        <label>Timeout</label>
        <property>virtualbox_timeout</property>
        <value></value>
        <documentation>Timeout for the step execution in minutes. Blank means no timeout.</documentation>
        <required>0</required>
        <type>entry</type>
    </formElement>
